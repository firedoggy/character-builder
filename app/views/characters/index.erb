<h1>Characters</h1>
<p><a href="/characters/new"><strong>Create a Character</strong></a></p><hr>
<% @characters.each do |character| %>
    <p>
        <a href="/characters/<%= character.id %>"><strong><%= character.name %></strong></a><br>
        <%= character.race %> <%= character.clas %>
    </p>
    <hr>
<% end %>